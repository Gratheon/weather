{
  "compilerOptions": {
    "module": "CommonJS",
    "target": "ES2017",
    "sourceMap": true,
    "outDir": "../../app",
    "baseUrl": "./",
    "strict": true,
    "allowJs": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "skipLibCheck": true,
    "strictNullChecks": false,
    "noImplicitAny": false,
    "resolveJsonModule": true
  },
  "include": ["../"],
  "exclude": ["../../node_modules", "../../app"]
}
